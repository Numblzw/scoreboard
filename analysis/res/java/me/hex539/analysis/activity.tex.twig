\documentclass[class=article]{standalone}
\usepackage[dvipsnames]{xcolor}
\usepackage{tikz}
\usepackage{pgfplots}
\usepackage{pgfplotstable}
\usetikzlibrary{patterns}
\pgfplotsset{compat=1.15}
\begin{document}
\pgfplotstableread{
t accept pending wronganswer timelimit failed{%
for y in accepted
%}
{{ loop.index-1 }} {{ y }} {{ pending[loop.index-1] }} {{ wronganswer[loop.index-1] }} {{ timelimit[loop.index-1] }} {{ failed[loop.index-1] }}{%
endfor
%}
}\attempts
\pgfplotsset{
	/pgfplots/bar cycle list/.style={/pgfplots/cycle list=
	{ {SkyBlue,fill=SkyBlue},% Pending 3
	  {Green,fill=LimeGreen},% Accepted
    {Red!50!OrangeRed,fill=OrangeRed},% Wrong-Answer
    {Red!50!Red,fill=Red},% Time-Limit
    {Red!50!Black!75,fill=Black!75},% Compile-Error / Runtime-Error
		{Blue!50!SkyBlue,fill=SkyBlue},% Pending 1
		{Blue!50!SkyBlue,fill=SkyBlue}}% Pending 2
	}
}%
\begin{tikzpicture}
  \begin{axis}[
    axis lines=none,
    xmin=0,
    xmax={{ (accepted|length) }},
    width={{ 20 }}cm,
    bar width={{ 15.0 / (accepted|length) }}cm,
    height=5cm,
    ybar stacked,
    grid=none,
    ticks=none]
    \addplot table[x=t,y expr={+0.01}] {\attempts};
    \addplot table[x=t,y expr={+\thisrow{accept}}] {\attempts};
    \addplot table[x=t,y expr={-\thisrow{wronganswer}}] {\attempts};
    \addplot table[x=t,y expr={-\thisrow{timelimit}}] {\attempts};
    \addplot table[x=t,y expr={-\thisrow{failed}}] {\attempts};
    \addplot table[x=t,y expr={+\thisrow{pending}/2}] {\attempts};
    \addplot table[x=t,y expr={-\thisrow{pending}/2}] {\attempts};
  \end{axis}
\end{tikzpicture}
\end{document}
